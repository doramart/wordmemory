# 应用配置
NODE_ENV=development
PORT=39010

# 应用密钥配置
APP_KEYS=your-app-name_1694000000000_1000

# 数据库配置
MONGODB_URL=mongodb://username:password@localhost:27017/database_name

# JWT配置
JWT_SECRET=your-jwt-secret-key-here
JWT_EXPIRES_IN=7d
JWT_ISSUER=pwa-scaffold-system

# 邮件服务配置
MAIL_SERVICE=smtp.qq.com
MAIL_PORT=465
MAIL_USER=your_email@qq.com
MAIL_PASS=your_email_auth_code
MAIL_FROM=单词速记 <your_email@qq.com>

# Doubao / DeepSeek 配置
DOUBAO_API_URL=
DOUBAO_API_KEY=
DOUBAO_VISION_MODEL=doubao-seed-1-6-vision-250815
DOUBAO_CHAT_MODEL=doubao-seed-1-6-251015
DOUBAO_IMAGE_MODEL=doubao-seedream-4-0-250828
DOUBAO_TIMEOUT_MS=45000

# 七牛云配置
QINIU_BUCKET=
QINIU_ACCESS_KEY=
QINIU_SECRET_KEY=
QINIU_DOMAIN=
QINIU_ZONE=Zone_z2

# 安全配置
CORS_ORIGIN=http://localhost:8080,http://127.0.0.1:8080
DOMAIN_WHITELIST=http://localhost:3000,http://127.0.0.1:3000,http://localhost:8080,http://127.0.0.1:8080

# 日志配置
LOG_LEVEL=INFO
LOG_DIR=logs

# 缓存配置
CACHE_TTL=3600

# 第三方服务配置（可选）
# EXTERNAL_API_URL=https://api.example.com
# EXTERNAL_API_KEY=your-api-key
